<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <!--Sequences for staff.member-->
        <record id="seq_staff_member" model="ir.sequence">
            <field name="name">Member Sequence</field>
            <field name="code">staff.member</field>
            <field name="prefix">SM</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--Sequences for staff.appointment-->
        <record id="seq_staff_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">staff.appointment</field>
            <field name="prefix">SA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>